			<parameterDescription><![CDATA[$parameters.parameterDescription]]></parameterDescription>
		</parameter>
	#end


	#* 
		Load Quey from the report definition repository
	
	*#
	<queryString>
		<![CDATA[$sql]]>
	</queryString>
	
