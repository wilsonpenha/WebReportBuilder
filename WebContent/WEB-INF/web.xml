<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>ReportBuilder</display-name>

	<resource-ref>
		<description>The default DS</description>
		<res-ref-name>jdbc/report_ds</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	<resource-ref>
		<description>The default DS</description>
		<res-ref-name>jdbc/sal_ds</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	<resource-ref>
		<description>The default DS</description>
		<res-ref-name>jdbc/mpp_ds</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	<!-- JDBC DataSources (java:comp/env/jdbc) 

	<resource-ref>
		<description>The default DS</description>
		<res-ref-name>jdbc/scoweb_ds</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>-->

	<context-param>
		<param-name>PARAMETER_ENCODING</param-name>
		<param-value>ISO-8859-1</param-value>
	</context-param>
	<servlet>
		<display-name>ReportServlet</display-name>
		<servlet-name>ReportServlet</servlet-name>
		<servlet-class>reports.util.ReportServlet</servlet-class>
		<init-param>
			<description></description>
			<param-name>velocity.properties</param-name>
			<param-value>/velocity.properties</param-value>
		</init-param>
		<load-on-startup>-1</load-on-startup>
	</servlet>
	<servlet>
		<display-name>GetFile</display-name>
		<servlet-name>GetFile</servlet-name>
		<servlet-class>reports.util.GetFile</servlet-class>
	</servlet>
	<servlet>
		<display-name>ImageServlet</display-name>
		<servlet-name>ImageServlet</servlet-name>
		<servlet-class>net.sf.jasperreports.j2ee.servlets.ImageServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>GraphicServlet</display-name>
		<servlet-name>GraphicServlet</servlet-name>
		<servlet-class>reports.util.GraphicServlet</servlet-class>
		<init-param>
			<param-name>velocity.properties</param-name>
			<param-value>/velocity.properties</param-value>
		</init-param>
	</servlet>
	<servlet>
		<display-name>AppletToServlet</display-name>
		<servlet-name>AppletToServlet</servlet-name>
		<servlet-class>reports.util.AppletToServlet</servlet-class>
		<load-on-startup>-1</load-on-startup>
	</servlet>
	<servlet>
		<display-name>PropertiesManager</display-name>
		<servlet-name>PropertiesManager</servlet-name>
		<servlet-class>
			br.com.hwork.servlet.PropertiesManager
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<display-name>PropertiesManagerAdmin</display-name>
		<servlet-name>PropertiesManagerAdmin</servlet-name>
		<servlet-class>
			br.com.hwork.servlet.admin.PropertiesManagerAdmin
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>action</servlet-name>
		<servlet-class>
			org.apache.struts.action.ActionServlet
		</servlet-class>
		<init-param>
			<param-name>config</param-name>
			<param-value>/WEB-INF/struts-config.xml</param-value>
		</init-param>
		<init-param>
			<param-name>chainConfig</param-name>
			<param-value>
				org/apache/struts/tiles/chain-config.xml
			</param-value>
		</init-param>
		<init-param>
			<param-name>debug</param-name>
			<param-value>2</param-value>
		</init-param>
		<init-param>
			<param-name>detail</param-name>
			<param-value>2</param-value>
		</init-param>
		<init-param>
			<param-name>validate</param-name>
			<param-value>true</param-value>
		</init-param>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetFile</servlet-name>
		<url-pattern>/getFile.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AppletToServlet</servlet-name>
		<url-pattern>/applets/applet2servlet.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReportServlet</servlet-name>
		<url-pattern>/factory.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ImageServlet</servlet-name>
		<url-pattern>/image</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GraphicServlet</servlet-name>
		<url-pattern>/graphic.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>action</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PropertiesManager</servlet-name>
		<url-pattern>/PropertiesManager</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PropertiesManagerAdmin</servlet-name>
		<url-pattern>/PropertiesManagerAdmin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>action</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<mime-mapping>
		<extension>htc</extension>
		<mime-type>text/x-component</mime-type>
	</mime-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<error-page>
		<error-code>400</error-code>
		<location>/jsp/common/errorpge.jsp</location>
	</error-page>
	<jsp-config>
		<taglib>
			<taglib-uri>test</taglib-uri>
			<taglib-location>test</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>hwork</taglib-uri>
			<taglib-location>/WEB-INF/tld/hwork.tld</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>
				http://jakarta.apache.org/taglibs/application-1.0
			</taglib-uri>
			<taglib-location>
				/WEB-INF/lib/taglibs-application.jar
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>
				http://jakarta.apache.org/taglibs/page-1.0
			</taglib-uri>
			<taglib-location>
				/WEB-INF/lib/taglibs-page.jar
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>
				http://jakarta.apache.org/taglibs/request-1.0
			</taglib-uri>
			<taglib-location>
				/WEB-INF/lib/taglibs-request.jar
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>
				http://jakarta.apache.org/taglibs/response-1.0
			</taglib-uri>
			<taglib-location>
				/WEB-INF/lib/taglibs-response.jar
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>
				http://jakarta.apache.org/taglibs/session-1.0
			</taglib-uri>
			<taglib-location>
				/WEB-INF/lib/taglibs-session.jar
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>jspsql</taglib-uri>
			<taglib-location>/WEB-INF/lib/jspsql.jar</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>
				http://jakarta.apache.org/taglibs/i18n-1.0
			</taglib-uri>
			<taglib-location>/WEB-INF/lib/i18n.jar</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>
				http://jakarta.apache.org/taglibs/datetime-1.0
			</taglib-uri>
			<taglib-location>
				/WEB-INF/lib/taglibs-datetime.jar
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>
				http://jakarta.apache.org/taglibs/string-1.0.1
			</taglib-uri>
			<taglib-location>
				/WEB-INF/lib/taglibs-string.jar
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>
				http://jakarta.apache.org/taglibs/utility
			</taglib-uri>
			<taglib-location>/WEB-INF/lib/utility.jar</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>
				http://jakarta.apache.org/taglibs/mailer-1.1
			</taglib-uri>
			<taglib-location>
				/WEB-INF/lib/taglibs-mailer.jar
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/tld/struts-tiles.tld</taglib-uri>
			<taglib-location>
				/WEB-INF/tld/struts-tiles.tld
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/tld/struts-bean.tld</taglib-uri>
			<taglib-location>
				/WEB-INF/tld/struts-bean.tld
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/tld/struts-html.tld</taglib-uri>
			<taglib-location>
				/WEB-INF/tld/struts-html.tld
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/tld/struts-logic.tld</taglib-uri>
			<taglib-location>
				/WEB-INF/tld/struts-logic.tld
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/tld/struts-nested.tld</taglib-uri>
			<taglib-location>
				/WEB-INF/tld/struts-nested.tld
			</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/tld/struts-template.tld</taglib-uri>
			<taglib-location>
				/WEB-INF/tld/struts-template.tld
			</taglib-location>
		</taglib>
	</jsp-config>
	<env-entry>
		<env-entry-name>PM_REFRESH_SPEED</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>12000</env-entry-value>
	</env-entry>
	<env-entry>
		<env-entry-name>PM_DIR_PATH</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>
			WEB-INF/classes/reports/resources
		</env-entry-value>
	</env-entry>
	<env-entry>
		<description>
			This should be relative to PM_DIR_PATH_ALTERNATE
		</description>
		<env-entry-name>WORK_LOC_FILE</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>worklocs/worklocs.txt</env-entry-value>
	</env-entry>
	<env-entry>
		<env-entry-name>REPORTS_CHAIN_CACHE_FILE</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>WEB-INF/reportsToData.txt</env-entry-value>
	</env-entry>
	<env-entry>
		<env-entry-name>
			PREFERENCES_OBJECT_MODEL_XMLFILE
		</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>
			WEB-INF/PreferencesObjectModel.xml
		</env-entry-value>
	</env-entry>
	<env-entry>
		<env-entry-name>RULES_XMLFILE</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>WEB-INF/Rules.xml</env-entry-value>
	</env-entry>
	<env-entry>
		<env-entry-name>PM_DIR_PATH_ALTERNATE</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>messages</env-entry-value>
	</env-entry>
	<env-entry>
		<description>
			YES or NO - Set to NO if running in local WSAD. If set to
			YES, should make PM_DIR_PATH_ALTERNATE set to full path
		</description>
		<env-entry-name>RUNNING_IN_WAS</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>NO</env-entry-value>
	</env-entry>
	<env-entry>
		<description>
		Auto Generated - SDO Datasource connection to jdbc:mysql://localhost:3306:mysql</description>
		<env-entry-name>jdbc/report_ds</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>jdbc:mysql:/localhost:3306/report,com.mysql.jdbc.Driver,WebReportBuilder</env-entry-value>
	</env-entry>
	<env-entry>
		<description>
		Auto Generated - SDO Datasource connection to jdbc:mysql://hadoopflex1:5432:postgressql</description>
		<env-entry-name>jdbc/mpp_ds</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>jdbc:postgressql://10.178.67.83:5432/ers,org.postgresql.Driver,WebReportBuilder</env-entry-value>
	</env-entry>
	<env-entry>
		<description>
		Auto Generated - SDO Datasource connection to Hive</description>
		<env-entry-name>jdbc/sal_ds</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>jdbc:hive2://10.178.67.81:10000/default;user=admin;password=admin;auth=noSasl,org.apache.hive.jdbc.HiveDriver,hive</env-entry-value>
	</env-entry>
</web-app>
